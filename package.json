{
  "name": "university-app",
  "version": "0.0.0-semantically-release",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "karma": "./node_modules/.bin/karma start --auto-watch",
    "lint": "./node_modules/.bin/gulp",
    "cclimate": "./node_modules/.bin/codeclimate-test-reporter",
    "test": "./node_modules/.bin/istanbul cover -x *.spec.js node_modules/.bin/mocha -- -R spec",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "init-cz": "./node_modules/.bin/commitizen init cz-conventional-changelog --save-dev --save-exact",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "ejs": "~2.5.5",
    "express": "~4.14.1",
    "morgan": "~1.7.0",
    "semantic-release-cli": "^3.0.3",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-run": "^1.7.1",
    "gulp-util": "^3.0.8",
    "istanbul": "^0.4.5",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "mocha": "^3.2.0",
    "run-sequence": "^1.2.2",
    "semantic-release": "^6.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodox/university.git"
  }
}
